{
  "name": "ts-types-guard",
  "version": "1.4.2",
  "description": "Util for runtime types checking for JS(TS)",
  "homepage": "https://github.com/Resetand/ts-types-guard",
  "author": "Resetand",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "guard",
    "typescript",
    "types",
    "is",
    "ts-guard",
    "ts-type-guards",
    "core-util-is",
    "lodash",
    "type-safe",
    "type-check",
    "validator",
    "type-guards",
    "joi",
    "schema"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.cjs",
    "dist/index.d.ts"
  ],
  "scripts": {
    "lint": "eslint --max-warnings=0 --ext=ts,tsx . --fix ",
    "build": "tsup",
    "test": "vitest --watch=false",
    "test:coverage": "npm run test -- --coverage",
    "prepublish": "npm run lint && npm run test && npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@babel/preset-typescript": "^7.23.3",
    "@swc/core": "^1.3.96",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "tsup": "^7.2.0",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  }
}
